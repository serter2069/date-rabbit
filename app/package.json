{
  "name": "app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "npx expo export --platform web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "generate:tests": "npx tsx scripts/generate-tests.ts",
    "test:web": "npm run generate:tests && npx playwright test",
    "test:web:logic": "LOGIC_TEST=true npm run test:web",
    "test:web:ui": "npm run generate:tests && npx playwright test --ui",
    "test:all": "npm test && npm run test:web"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo/metro-runtime": "~4.0.1",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/slider": "4.5.5",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "expo": "52",
    "expo-constants": "~17.0.8",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image": "~2.0.7",
    "expo-image-picker": "~16.0.6",
    "expo-linking": "~7.0.5",
    "expo-location": "^19.0.8",
    "expo-notifications": "^0.32.16",
    "expo-router": "~4.0.22",
    "expo-status-bar": "~2.0.1",
    "expo-web-browser": "~14.0.2",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.17",
    "lucide-react-native": "^0.563.0",
    "patch-package": "^8.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@anthropic-ai/bedrock-sdk": "^0.26.3",
