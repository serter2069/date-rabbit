    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.1",
    "jest-expo": "^54.0.17",
    "magnitude-test": "^0.3.13",
    "react-test-renderer": "^19.1.0",
    "tsx": "^4.21.0",
    "typescript": "~5.9.2",
    "yaml": "^2.8.2"
  },
